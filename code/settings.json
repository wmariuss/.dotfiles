{
  "editor.snippetSuggestions": "none",
  "editor.tabCompletion": "on",
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 4,
  "workbench.colorCustomizations": {
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238"
  },
  "workbench.startupEditor": "newUntitledFile",
  "todo.symbols.box": "â–ª",
  "workbench.activityBar.visible": true,
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "*.egg-info": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.coverage": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.tox": true,
    "**/.venv": true,
    "**/.vscode": true,
    "**/*.py[co]": true,
    "**/.terraform": true,
    "vendor": true
  },
  "workbench.statusBar.feedback.visible": false,
  "window.zoomLevel": 0,
  "python.venvPath": "/Users/mariuss/.local/share/virtualenvs",
  "python.pipenvPath": "/Users/mariuss/.pyenv/shims/pipenv",
  "python.pythonPath": "${workspaceFolder}/.venv/bin/python3",
  "python.linting.pep8Enabled": true,
  "python.linting.pylintEnabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintArgs": ["--max-line-length=180"],
  "python.linting.pep8Args": ["--max-line-length=180"],
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": true,
  "python.envFile": "${workspaceFolder}/.env",
  "diffEditor.ignoreTrimWhitespace": true,
  "files.insertFinalNewline": true
}

