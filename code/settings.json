{
  "editor.snippetSuggestions": "none",
  "editor.tabCompletion": "on",
  "editor.glyphMargin": true,
  "editor.minimap.enabled": false,
  "editor.insertSpaces": true,
  "editor.fontSize": 12,
  "editor.tabSize": 4,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.formatOnSave": true,
  "workbench.startupEditor": "none",
  "todo.symbols.box": "â–ª",
  "workbench.activityBar.visible": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "*.egg-info": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.coverage": true,
    "**/.DS_Store": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.terraform": true,
    "**/.tox": true,
    "**/.venv": true,
    "**/.vscode": true,
    "**/*.py[co]": true,
    "vendor": true
  },
  "window.zoomLevel": 0,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "python.linting.enabled": true,
  "python.linting.pylintPath": "pylint",
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    "--max-line-length=120"
  ],
  "python.venvPath": "/home/mariuss/.local/share/virtualenvs",
  "python.pipenvPath": "/home/mariuss/.pyenv/shims/pipenv",
  "python.pythonPath": "/home/mariuss/.pyenv/shims/python",
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "/home/mariuss/.pyenv/shims/mypy",
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": true,
  "python.envFile": "${workspaceFolder}/.env",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/home/mariuss/.pyenv/shims/black",
  "python.formatting.blackArgs": [
    "--line-length",
    "90"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "files.insertFinalNewline": true,
  "workbench.statusBar.visible": true,
  "workbench.statusBar.feedback.visible": false,
  "[python]": {
    "editor.formatOnPaste": false
  },
  "workbench.iconTheme": null,
  "editor.fontLigatures": true,
  "editor.lineHeight": 21,
  "editor.fontFamily": "Fira Code, Monaco, 'Courier New', monospace",
  "workbench.colorTheme": "Hop Light",
  "projectManager.groupList": true,
  "projectManager.sortList": "Name"
}

